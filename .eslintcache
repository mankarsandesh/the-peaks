chTopNew(e.target.value);\n    this.fetchSports(e.target.value);\n  }\n\n  render() {\n    const {\n      error,\n      isLoaded,\n      items,\n      categoryData,\n      isLoadedCategory,\n    } = this.state;\n\n    return (\n      <div className=\"App-cotainer\">\n        <div className=\"container-header\">\n          <h1>Top stories</h1>\n          <div className=\"container-header-right\">\n            <Link to=\"/allbookmark\">\n              <button className=\"bookmark\">\n                <i className=\"fa fa-bookmark\"></i>View Bookmark\n              </button>\n            </Link>\n            <select\n              className=\"search\"\n              value={this.state.selectType}\n              onChange={this.selectNewsType}\n            >\n              <option value=\"newest\">Newest first</option>\n              <option value=\"oldest\">Oldest first</option>\n              <option value=\"relevance\">Most Popular</option>\n            </select>\n          </div>\n        </div>\n        <div className=\"container\">\n          <div className=\"container-left\">\n            {items.slice(0, 1).map((item) => (\n              <Article\n                key={item.fields.id}\n                mediaTitle={item.fields.headline}\n                mediaDesc={item.fields.trailText}\n                media={item.fields.thumbnail}\n                articleURL={item.id}\n                type={item.sectionId}\n              />\n             \n            ))}\n          \n          </div>\n          <div className=\"container-right\">\n            {items.slice(1, 3).map((item) => (\n              <div className=\"col-6\">\n                <Article\n                  key={item.fields.id}\n                  mediaTitle={item.fields.headline}\n                  media={item.fields.thumbnail}\n                  articleURL={item.id}\n                  height={\"300px\"}\n                />\n              </div>\n            ))}\n\n            {items.slice(4, 6).map((item) => (\